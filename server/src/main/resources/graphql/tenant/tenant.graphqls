type Tenant {
  id: ID!
  name: String
  email: String
  avatar: String
  createdAt: LocalDateTime
  applications: [Application]
  payments: [Payment]
}

extend type Query {
  getTenant(id: ID!): Tenant
  getAuthenticatedTenant: Tenant
  getCreatedApplications: [Application]
}

extend type Mutation {
    createApplication(input: CreateApplicationDto) : Application
}
