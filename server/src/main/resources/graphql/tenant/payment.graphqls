enum PaymentStatus {
  PENDING
  PAID
  PARTIALLY_PAID
  OVERDUE
}

type Payment {
  id: ID!
  amountDue: Float
  amountPaid: Float
  dueDate: LocalDateTime
  paymentDate: LocalDateTime
  status: PaymentStatus
  tenant: Tenant
  propertyTenantPaymentApplication: PropertyTenantPaymentApplication
}

extend type Query {
    getPayment(id: ID!): Payment
}
